{
  "name": "use-this-search",
  "version": "1.0.0",
  "description": "AI-Powered Keyword Research SaaS Platform",
  "main": "backend/server.py",
  "scripts": {
    "build": "cd frontend && yarn install --frozen-lockfile && yarn build",
    "start": "cd backend && uvicorn server:app --host 0.0.0.0 --port $PORT",
    "dev": "cd backend && uvicorn server:app --reload",
    "install-backend": "cd backend && pip install -r requirements.txt",
    "install-frontend": "cd frontend && yarn install"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {},
  "devDependencies": {}
}